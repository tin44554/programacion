class Persona:
    nombre = ""
    edad = 0 
    altura = 0
    genero = ""
    
    def caminar(self):
        return f"Hola, soy {self.nombre}"
        
    def hablar(self):
        return "Me gusta estar en clase"


# Crear instancias fuera de la clase
persona1 = Persona()
persona2 = Persona()
persona3 = Persona()

# Asignar valores correctos
persona1.nombre = "Gonzalo"
persona1.edad = 108
persona1.altura = 178
persona1.genero = "masculino"

persona2.nombre = "Maria"
persona2.edad = 198
persona2.altura = 143
persona2.genero = "femenino"

persona3.nombre = "Andres"
persona3.edad = 78
persona3.altura = 180
persona3.genero = "masculino"

# Imprimir atributos y llamar m√©todos
print(persona1.nombre)
print(persona1.edad)
print(persona1.altura)
print(persona1.genero)

print(persona2.nombre)
print(persona2.edad)
print(persona2.altura)
print(persona2.genero)

print(persona3.nombre)
print(persona3.edad)
print(persona3.altura)
print(persona3.genero)

print(persona1.caminar())
print(persona1.hablar())

print(persona2.caminar())
print(persona2.hablar())

print(persona3.caminar())
print(persona3.hablar())
